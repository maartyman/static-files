@prefix snvoc: <http://localhost:3000/www.ldbc.eu/ldbc_socialnet/1.0/vocabulary/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

{
    ?x a snvoc:Person.
}
=>
{
    ?x a foaf:Person.
}.

{
    ?x snvoc:birthday ?v.
}
=>
{
    ?x foaf:birthday ?v.
}.

{
    ?x snvoc:firstName ?v.
}
=>
{
    ?x foaf:firstName ?v.
}.

{
    ?x snvoc:gender ?v.
}
=>
{
    ?x foaf:gender ?v.
}.

{
    ?x snvoc:hasInterest ?v.
}
=>
{
    ?x foaf:interest ?v.
}.

{
    ?x snvoc:lastName ?v.
}
=>
{
    ?x foaf:family_name ?v.
}.

{
    ?x snvoc:email ?v.
}
=>
{
    ?x foaf:mbox ?v.
}.

{
    ?x snvoc:knows ?n.
    ?n snvoc:hasPerson ?y.
}
=>
{
    ?x foaf:knows ?y.
}.

{
    ?x snvoc:studyAt ?n.
    ?n snvoc:hasOrganisation ?y.
}
=>
{
    ?x foaf:schoolHomepage ?y.
}.

{
    ?x snvoc:workAt ?n.
    ?n snvoc:hasOrganisation ?y.
}
=>
{
    ?x foaf:workInfoHomepage ?y.
}.
